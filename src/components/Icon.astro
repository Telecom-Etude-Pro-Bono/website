---
const { icon, alt } = Astro.props;
interface IconProps {
    icon: string;
    alt: string;
}
---

<img class="icon" alt={alt} src={"/icons/" + icon + ".svg"} />
<style>
    .icon {
        position: relative;
        width: -webkit-fill-available;
        height: -webkit-fill-available;
    }
</style>
